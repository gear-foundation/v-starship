{
  "name": "squid",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "processor:run": "node -r dotenv/config lib/main.js",
    "migration:generate": "typeorm migration:generate -d lib/model/data-source.js -o db/migrations/$NAME",
    "migration:run": "typeorm migration:run -d lib/model/data-source.js",
    "serve": "node lib/api.js"
  },
  "dependencies": {
    "@subsquid/substrate-processor": "8.6.1",
    "@subsquid/typeorm-store": "1.5.1",
    "dotenv": "17.2.1",
    "express": "5.1.0",
    "pg": "8.16.3",
    "postgraphile": "4.14.1",
    "postgraphile-plugin-connection-filter": "2.3.0",
    "reflect-metadata": "0.2.2",
    "sails-js": "0.4.3",
    "sails-js-parser": "0.4.3",
    "typeorm": "0.3.26"
  },
  "devDependencies": {
    "@subsquid/substrate-metadata-explorer": "3.2.0",
    "@types/express": "5.0.3",
    "@types/node": "24.3.0",
    "typescript": "5.9.2"
  }
}
